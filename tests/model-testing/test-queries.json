  "testProfiles": {
    "quick": ["simple-math", "factorial", "current-events", "prime-numbers"],
    "comprehensive": [
      "simple-math", "general-knowledge", "definition",
      "factorial", "complex-calculation", "fibonacci",
      "current-events", "recent-tech",
      "prime-numbers", "algorithm",
      "logic-puzzle", "compound-interest",
