          "expectedAnswer": null,
          "validateFunction": "has_expert_persona",
          "expectedPersonaDomain": "physics"
        },
        {
          "id": "medical-expert",
          "query": "What are the main differences between Type 1 and Type 2 diabetes in terms of pathophysiology and treatment?",
          "expectedAnswer": null,
          "validateFunction": "has_expert_persona",
          "expectedPersonaDomain": "medical"
        },
        {
          "id": "economics-expert",
          "query": "Explain the concept of quantitative easing and its effects on inflation and employment.",
          "expectedAnswer": null,
          "validateFunction": "has_expert_persona",
          "expectedPersonaDomain": "economics"
        }
      ]
