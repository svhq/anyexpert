    ],
    "tools-focus": [
      "factorial", "fibonacci",
      "current-events", "recent-tech",
      "prime-numbers", "data-analysis"
    ],
    "reasoning-focus": [
      "logic-puzzle", "compound-interest", "problem-solving",
      "physics-expert", "medical-expert", "economics-expert"
    ]
  },
  "validationFunctions": {
    "exact_match": "response === expected",
    "contains": "response.toLowerCase().includes(expected.toLowerCase())",
    "contains_concept": "key concepts are present",
    "has_search_results": "response includes web search results",
    "contains_last_prime": "response includes the highest prime",
    "has_statistics": "response includes mean, median, and std dev",
    "contains_logic": "response includes logical reasoning",
    "contains_months": "response includes time calculation",
    "contains_speed": "response includes average speed",
    "has_expert_persona": "response includes named expert"
  }
