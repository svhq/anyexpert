{
  "categories": {
    "basic": {
      "name": "Basic Queries",
      "description": "Simple questions that don't require tools",
      "queries": [
        {
          "id": "simple-math",
          "query": "What is 25 × 4?",
          "expectedAnswer": "100",
          "validateFunction": "exact_match"
        },
        {
          "id": "general-knowledge",
          "query": "Who wrote Romeo and Juliet?",
          "expectedAnswer": "William Shakespeare",
          "validateFunction": "contains"
        },
        {
          "id": "definition",
          "query": "What is photosynthesis?",
          "expectedAnswer": "process by which plants use sunlight",
          "validateFunction": "contains_concept"
        }
      ]
    },
    "math": {
      "name": "Mathematical Computation",
      "description": "Queries requiring mathematical calculation",
      "queries": [
        {
          "id": "factorial",
          "query": "Calculate the factorial of 15 and show me the exact result.",
          "expectedAnswer": "1307674368000",
          "validateFunction": "exact_match",
          "requiresExecution": true
        },
        {
          "id": "complex-calculation",
          "query": "Calculate e^π - π^e and determine which is larger. Show exact values with 10 decimal places.",
          "expectedAnswer": "e^π is larger",
          "validateFunction": "contains",
          "requiresExecution": true
        },
        {
          "id": "fibonacci",
          "query": "Generate the first 20 Fibonacci numbers and calculate their sum.",
          "expectedAnswer": "10946",
          "validateFunction": "contains",
          "requiresExecution": true
        }
      ]
    },
    "search": {
      "name": "Web Search Queries",
      "description": "Queries requiring current information from the web",
      "queries": [
        {
          "id": "current-events",
          "query": "What are the latest developments in quantum computing in 2025?",
          "expectedAnswer": null,
          "validateFunction": "has_search_results",
          "requiresSearch": true
        },
        {
          "id": "recent-tech",
          "query": "What new AI models were released in the last month?",
          "expectedAnswer": null,
          "validateFunction": "has_search_results",
          "requiresSearch": true
        },
        {
          "id": "trending-topics",
          "query": "What are the current trending topics in machine learning research?",
          "expectedAnswer": null,
          "validateFunction": "has_search_results",
          "requiresSearch": true
        }
      ]
    },
    "code": {
      "name": "Code Generation and Execution",
      "description": "Queries requiring code writing and execution",
      "queries": [
        {
          "id": "prime-numbers",
          "query": "Write and execute Python code to find all prime numbers under 100.",
          "expectedAnswer": "97",
          "validateFunction": "contains_last_prime",
          "requiresCodeExecution": true
        },
        {
          "id": "data-analysis",
          "query": "Create a Python script that generates 10 random numbers between 1 and 100, then calculates their mean, median, and standard deviation.",
          "expectedAnswer": null,
          "validateFunction": "has_statistics",
          "requiresCodeExecution": true
        },
        {
          "id": "algorithm",
          "query": "Implement bubble sort in Python and demonstrate it sorting the list [64, 34, 25, 12, 22, 11, 90].",
          "expectedAnswer": "[11, 12, 22, 25, 34, 64, 90]",
          "validateFunction": "contains",
          "requiresCodeExecution": true
        }
      ]
    },
    "reasoning": {
      "name": "Complex Reasoning",
      "description": "Multi-step reasoning and logic problems",
      "queries": [
        {
          "id": "logic-puzzle",
          "query": "If all roses are flowers and some flowers fade quickly, can we conclude that some roses fade quickly? Explain your reasoning.",
          "expectedAnswer": "No",
          "validateFunction": "contains_logic"
        },
        {
          "id": "compound-interest",
          "query": "Calculate the compound interest on $10,000 at 5% annual rate for 3 years, then determine how many months it would take to double at that rate.",
          "expectedAnswer": "167",
          "validateFunction": "contains_months"
        },
        {
          "id": "problem-solving",
          "query": "A train travels from City A to City B at 60 mph and returns at 40 mph. What is the average speed for the entire trip?",
          "expectedAnswer": "48",
          "validateFunction": "contains_speed"
        }
      ]
    },
    "expert": {
      "name": "Expert Domain Knowledge",
      "description": "Queries testing expert persona and domain knowledge",
      "queries": [
        {
          "id": "physics-expert",
